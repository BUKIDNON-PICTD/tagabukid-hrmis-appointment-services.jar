	def em

	@Service("PersistenceService")
    def persistenceSvc;

	@Service('DateService')
	def dtSvc 

	@Env
	def env;

	@ProxyMethod
	public def getPersonnelPlantilla(params){
		// println params
		def data = [:]
		// def items = []
		def items = em.getResult([funnel:params.org.orgunitid])
		
		// params.funnel = params.orgunitid
		// items = em.getResult(params)
		// def PlantillaPersonnels = []
		// 	items.plantillapersonnel.each{g ->
		// 		PlantillaPersonnels.add(g)
		// 	}
		// items.plantillapersonnel = items
		
		data.reportdata = items;
		println items
		
		data.parameters = [
			TITLE          	: "PERSONNEL PLANTILLA",
			YEAR         	: dtSvc.getServerYear().toString(),
			PROVINCE       	: "Bukidnon",
			OFFICE			: params.name,
			// TRANCHE_NAME	: tranchename[0].name,
			// TRANCHE_CODE	: tranchename[0].circular,
			// TRANCHE_PREVYEAR: tranchename[0].prevyear.toString()
